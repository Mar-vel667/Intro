#include <iostream>
using namespace std;

int main() {
    float celsius;  // input temperature in Celsius
    float fahrenheit;  // result in Fahrenheit

    cout << "Please enter degree in Celsius: ";
    cin >> celsius;

    fahrenheit = (celsius * 9.0 / 5.0) + 32;

    cout << "Fahrenheit: " << fahrenheit << endl;

    return 0;
}
